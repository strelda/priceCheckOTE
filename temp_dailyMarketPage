</div>
            
                <h2 class="documentFirstHeading">Intra-Day Market</h2>
            
            <div id="viewlet-below-content-title"></div>

            
                
            
          </header>

          <section id="viewlet-above-content-body"></section>
          <section id="content-core">
            
        <div>
            
                <!-- input parameters -->
                <div class="datepickandsubmit report_input">
                    
                    
                    	<!-- report date -->
                        <span class="glyphicon glyphicon-chevron-left previous-day"></span>
			    		<input type="text" class="datepicker-here" readonly="readonly" data-min-view="days" data-view="days" />
			    		<span class="glyphicon glyphicon-chevron-right next-day"></span>
                    
                    
                    

                    

                    

                                        
                    

                </div>
                
                <!-- content -->
                <div class="report_content">
                    
                    	
            
                            <script type="text/javascript">

                var dataUrl = "./json.php?uri=" + encodeURI("https://www.ote-cr.cz/pubweb/cs/kratkodobe-trhy/elektrina/vnitrodenni-trh/@@chart-data");

                //if true, graph objects are displayed, otherwise not
                var displayGraph = true;
                ////server defined absolute url for chart data. Overwrites Javascript's generated URL. Do not define value, if not used.
                var serverDataUrl = "/en/short-term-markets/electricity/intra-day-market/@@chart-data?report_date=2022-04-27";
                //minimal date for data component - do not define value, if not used
                var minDate = new Date(2002, 01 - 1, 01);
                //maximal date for data component - do not define value, if not used
                var maxDate = new Date(2022, 04 - 1, 27);

                var datepickerParams = {
                    language: "en",
                };

                //selected date in datepicker - do not define value, if not used
                var defaultSelectDate = new Date(2022, 04 - 1, 27);


                function loadDataForDate(date) {

                                var d = date.getDate();
                                var m = date.getMonth() + 1;
                                var y = date.getFullYear();
                                var parsableDate = y + '-' + (m <= 9 ? '0' + m : m) + '-' + (d <= 9 ? '0' + d : d);

                                history.replaceState({}, '', changeGetParameterInUrl(window.location.href, "date", parsableDate));

                                dataUrl = dataUrl + "?" + $.param({"report_date": parsableDate});

                                if (serverDataUrl !== undefined) {
                                    dataUrl = serverDataUrl;
                                }

                                if (displayGraph) {
                                    $.get(dataUrl, function (response) {

                                        var axisX = [];
                                        var axisY = [];
                                        var axisXlabel = response['axis']['x']['legend'];
                                        var axisYlabels = [
                                            response['axis']['y']['legend'],
                                            response['axis']['y2']['legend'],
                                        ];
                                        var axisYnames = [];
                                        var axisIds = [];



                                        var largestAxisSize = 0;
                                        var largestAxisIndex = 0;
                                        axisY.forEach(function (object, index) {
                                            var axisSize = axisY[index].length;

                                            if (largestAxisSize < axisSize) {
                                                largestAxisSize = axisSize;
                                                largestAxisIndex = index;
                                            }
                                        });

                                        var destIndex = 0;

                                        response['data']['dataLine'].forEach(function (axisObject, axisIndex) {
                                            if (axisIndex == largestAxisIndex) {
                                                axisObject['point'].forEach(function (object) {
                                                    axisX.push(object['x']);
                                                });
                                            }
                                            if (1) {
                                                // mnozstvi has to be last because of z-index
                                                if (axisIndex == 0)
                                                {
                                                    destIndex = 3;
                                                } else if (axisIndex == 3)
                                                {
                                                    destIndex = 0;
                                                } else {
                                                    destIndex = axisIndex;
                                                }
                                            } else {
                                                destIndex = axisIndex;

                                            }

                                            if (axisObject['useY2']) {
                                                axisIds[destIndex] = 'second-y-axis';
                                            } else {
                                                axisIds[destIndex] = 'first-y-axis';
                                            }
                                            axisYnames[destIndex] = axisObject['title'];

                                            var tempData = [];
                                            axisObject['point'].forEach(function (object, index) {
                                                tempData.push(object['y']);
                                            });

                                            axisY[destIndex] = tempData;
                                        });



                                        axisY.forEach(function (object, index) {
                                            var axisSize = axisY[index].length;
                                            if (axisSize < largestAxisSize) {
                                                for (var i = 0; i < (largestAxisSize - axisSize); i++) {
                                                    axisY[index].push(null);
                                                }

                                            }
                                        });

                                        renderGraph(axisY, axisX, axisXlabel, axisYlabels, axisYnames, axisIds);

                                    });
                                } else {
                                    $('.chart').hide();
                                }

                            }

                            $(document).ready(function () {

                                function changeUrlDateAndReload(date) {

                                    var d = date.getDate();
                                    var m = date.getMonth() + 1;
                                    var y = date.getFullYear();
                                    var parsable = y + '-' + (m <= 9 ? '0' + m : m) + '-' + (d <= 9 ? '0' + d : d);

                                    document.location.href = changeGetParameterInUrl(window.location.href, "date", parsable);

                                }

                                var hammertime = new Hammer(window);

                                $('.datepickandsubmit>.datepicker-here').datepicker({
                                    dateFormat: "d. MM yyyy",
                                    autoClose: true,
                                    onSelect: function (fd, date, picker) {
                                        if ($_GET("date") === null || new Date($_GET("date")).getTime() == date.getTime()) {
                                            loadDataForDate(date);
                                        } else {
                                            changeUrlDateAndReload(date);
                                        }
                                    },
                                    onShow: function (dp, animationCompleted) {
                                        if (!animationCompleted) {

                                            hammertime.on('tap', function (ev) {
                                                if (ev.pointerType === 'touch') {
                                                    $('.datepickandsubmit>.datepicker-here').datepicker().data('datepicker').hide();
                                                }
                                            });
                                        }
                                    },
                                    onHide: function (dp, animationCompleted) {
                                        if (!animationCompleted) {
                                            hammertime.off('tap');
                                        }
                                    },
                                });

                                $('.datepickandsubmit > .previous-day').bind('click', function () {

                                    var datepicker = $(this).parent().children('input').data('datepicker');

                                    var selectedDate = datepicker.lastSelectedDate;

                                    selectedDate = addDays(selectedDate, -1);
                                    datepicker.selectDate(selectedDate);
                                });

                                $('.datepickandsubmit > .next-day').bind('click', function () {

                                    var datepicker = $(this).parent().children('input').data('datepicker');

                                    var selectedDate = datepicker.lastSelectedDate;

                                    selectedDate = addDays(selectedDate, 1);
                                    datepicker.selectDate(selectedDate);
                                });

                                $('.datepickandsubmit > .open-icon').bind('click', function () {

                                    $(this).parent().children('input').data('datepicker').show();
                                });

                                var selectedDate = $_GET("date");
                                if (selectedDate == null) {
                                    if (defaultSelectDate !== undefined) {
                                        selectedDate = defaultSelectDate;
                                    } else {
                                        selectedDate = new Date();
                                    }

                                } else {
                                    selectedDate = new Date(Date.parse(selectedDate));

                                    if (isNaN(selectedDate.getTime())) {
                                        if (defaultSelectDate !== undefined) {
                                            selectedDate = defaultSelectDate;
                                        } else {
                                            selectedDate = new Date();
                                        }
                                    }
                                }

                                $('.datepickandsubmit>input').data('datepicker').selectDate(selectedDate);
                                $('.datepickandsubmit>input').data('datepicker').date = selectedDate;

                                if (minDate !== undefined) {
                                    datepickerParams.minDate = minDate;
                                }

                                if (maxDate !== undefined) {
                                    datepickerParams.maxDate = maxDate;
                                }

                                $('.datepickandsubmit>input').data('datepicker').update(datepickerParams);

                            });
                            </script>

    <!-- chart -->
    <div>
                        <h3 class="chart_title">Intra-Day Market Results - 27.04.2022</h3>
                        <div id="chartjs-legend" class="noselect"></div>
                        <canvas id="bigchart" class="chart"></canvas>
                        <style type="text/css">
                            canvas{
                                -moz-user-select: none;
                                -webkit-user-select: none;
                                -ms-user-select: none;
                            }
                            #chartjs-tooltip {
                                opacity: 1;
                                position: absolute;
                                background: rgba(0, 0, 0, .8);
                                color: white;
                                -webkit-transition: all .1s ease;
                                transition: all .1s ease;
                                pointer-events: none;
                                -webkit-transform: translate(-50%, 0);
                                transform: translate(-50%, 0);
                                width: auto;
                                /*max-width: 240px;
                                min-width: 230px;*/
                            }
                            #chartjs-tooltip>table{
                                width: 100%;
                            }

                            .chartjs-tooltip-key {
                                display: inline-block;
                                width: 10px;
                                height: 10px;
                                margin-right: 10px;
                            }
                        </style>
                        <script type="text/javascript">
                            function renderGraph(myData, myLabelData, myAxisXlabel, myAxisYlabels, myAxisYnames, myAxisIds) {
                                var ctx = document.getElementById("bigchart").getContext('2d');

                                var splitDataY1ForAxisLimit = [];
                                var splitDataY2ForAxisLimit = [];


                                myData.forEach(function (axisObject, axisIndex) {

                                    if (myAxisIds[axisIndex] == 'first-y-axis') {
                                        splitDataY1ForAxisLimit.push(axisObject);
                                    } else {
                                        splitDataY2ForAxisLimit.push(axisObject);
                                    }
                                });

                                var axisLimits = getAxesLimitsForDataY1Y2(splitDataY1ForAxisLimit, splitDataY2ForAxisLimit, 8, 1.15);

                                var axisTemplates = [
                                    {
                                        borderColor: 'rgba(255, 51, 0, 1)',
                                        backgroundColor: 'rgba(255, 255, 255, 1)',
                                        borderWidth: 2,
                                        fill: false,
                                        type: 'line',
                                        lineTension: 0,
                                    },
                                    {
                                        borderColor: 'rgba(77,181,189, 1)',
                                        backgroundColor: 'rgba(77,181,189, 1)',
                                        borderWidth: 2,
                                        fill: false,
                                        type: 'line',
                                        lineTension: 0,
                                    },
                                    {
                                        backgroundColor: 'rgba(217, 181, 38, 1)',
                                        borderColor: 'rgba(217, 181, 38, 1)',
                                        borderWidth: 2,
                                        fill: false,
                                        type: 'line',
                                        lineTension: 0,
                                    },
                                    {
                                        backgroundColor: 'rgba(0, 51, 153, 1)',
                                        borderWidth: 0,

                                    },
                                ];

                                var myChart = new Chart(ctx, {
                                    type: 'bar',
                                    data: {
                                        labels: myLabelData,
                                        datasets: []
                                    },
                                    options: {
                                        legendCallback: function (chart) {
                                            var text = [];
                                            text.push('<ul class="' + chart.id + '-legend" style="list-style-type: none; padding-left: 0px;">');
                                            
                                            for (var i = 0; i < chart.data.datasets.length; i++) {
                                                var axis = chart.data.datasets[i];
                                                text.push('<li data-datasetIndex="' + chart.legend.legendItems[i].datasetIndex + '" onclick="updateDataset(event, ' + '\'' + chart.legend.legendItems[i].datasetIndex + '\'' + ')" class="legend-li">');
                                                if (axis['type'] == "line") {
                                                    text.push('<div style="width: 40px; height: 15px; margin-right: 5px;border:1px solid #CCCCCC;display: inline-block;">\n\
                                <div style="height: 3px; width: 12px; background: ' + axis['borderColor'] + '; float: left; margin: 5px 0px 0px 3px;"></div>\n\
<div style="border-radius: 50px; border: 2px solid ' + axis['borderColor'] + '; background: ' + axis['backgroundColor'] + '; height: 8px; width: 8px;margin: 3px 0px 0px 0px;float: left;"></div>\n\
<div style="height: 3px; width: 12px; background: ' + axis['borderColor'] + ';float: right;margin: 5px 3px 0px 0px;"></div>\n\
\n\
\n\</div>');
                                                } else {
                                                    text.push('<div style="display: inline-block; width: 40px; height: 15px; margin-right: 5px; background-color:' + axis['backgroundColor'] + '"></div>');
                                                }
                                                if (chart.data.datasets[i].label) {
                                                    text.push('<span class="axis-name">' + chart.data.datasets[i].label + '</span>');
                                                }
                                                text.push('</li>');
                                            }
                                            text.push('</ul>');
                                            return text.join("");
                                        },
                                        legend: {
                                            display: false,
                                        },
                                        tooltips: {
                                            enabled: false,
                                            mode: 'index',
                                            intersect: true,
                                            custom: function (tooltip) {
                                                var chart = this._chart;
                                                var tooltipEl = document.getElementById('chartjs-tooltip');
                                                if (!tooltipEl) {
                                                    tooltipEl = document.createElement('div');
                                                    tooltipEl.id = 'chartjs-tooltip';
                                                    tooltipEl.innerHTML = "<table></table>"
                                                    this._chart.canvas.parentNode.appendChild(tooltipEl);
                                                }

                                                // Hide if no tooltip
                                                if (tooltip.opacity === 0) {
                                                    tooltipEl.style.opacity = 0;
                                                    return;
                                                }

                                                // Set caret Position
                                                tooltipEl.classList.remove('above', 'below', 'no-transform');
                                                if (tooltip.yAlign) {
                                                    tooltipEl.classList.add(tooltip.yAlign);
                                                } else {
                                                    tooltipEl.classList.add('no-transform');
                                                }

                                                function getBody(bodyItem) {
                                                    return bodyItem.lines;
                                                }


                                                // Set Text
                                                if (tooltip.body) {
                                                    var titleLines = tooltip.title || [];
                                                    var bodyLines = tooltip.body.map(getBody);
                                                    var innerHtml = '<thead>';
                                                    titleLines.forEach(function (title) {
                                                        innerHtml += '<tr><th>' + title + '</th></tr>';
                                                    });
                                                    innerHtml += '</thead><tbody>';

                                                    var correctDatasetIds = [];
                                                    chart.legend.legendItems.forEach(function (legendItem, legendIndex) {
                                                        if (legendItem.hidden == false) {
                                                            correctDatasetIds.push(legendIndex);
                                                        }
                                                    });

                                                    var xIndex = tooltip.dataPoints[0].index;
                                                    chart.tooltip._data.datasets.forEach(function (dataset, datasetIndex) {


                                                        var value = dataset.data[xIndex];
                                                        var isHidden = chart.legend.legendItems[datasetIndex].hidden;
                                                        
                                                        if (value === undefined || isHidden === true) {
                                                        } else {

                                                            var style = 'background:' + dataset.backgroundColor;
                                                            style += '; border-color:' + dataset.borderColor;
                                                            style += '; border-width: 2px';
                                                            var axis = dataset;
                                                            if (axis['type'] == "line") {
                                                                var span = '<div style="width: 40px; height: 15px; margin-right: 5px;border:1px solid #CCCCCC;display: inline-block;background: white;"><div style="height: 3px; width: 12px; background: ' + axis['borderColor'] + '; float: left; margin: 5px 0px 0px 3px;"></div><div style="border-radius: 50px; border: 2px solid ' + axis['borderColor'] + '; background: ' + axis['backgroundColor'] + '; height: 8px; width: 8px;margin: 3px 0px 0px 0px;float: left;"></div><div style="height: 3px; width: 12px; background: ' + axis['borderColor'] + ';float: right;margin: 5px 3px 0px 0px;"></div></div>';
                                                            } else {
                                                                var span = '<div style="width: 40px; height: 15px; margin-right: 5px;border:1px solid #CCCCCC;display: inline-block;background-color:' + axis['backgroundColor'] + ';"></div>';
                                                            }

                                                            if (axis['yAxisID'] == 'first-y-axis') {
                                                                var decimalsLength = 2;
                                                            } else {
                                                                var decimalsLength = 1;
                                                            }


                                                            innerHtml += '<tr><td>' + span + axis['label'] + ':&nbsp;&nbsp;' + formatNumber(value, decimalsLength, ',', ' ') + '</td></tr>';
                                                        }
                                                    });
                                                    innerHtml += '</tbody>';
                                                    var tableRoot = tooltipEl.querySelector('table');
                                                    tableRoot.style.border = "0px";
                                                    tableRoot.innerHTML = innerHtml;
                                                }

                                                var positionY = this._chart.canvas.offsetTop;
                                                var positionX = this._chart.canvas.offsetLeft;
                                                // Display, position, and set styles for font
                                                tooltipEl.style.opacity = 1;
                                                tooltipEl.style.left = positionX + tooltip.caretX + 'px';
                                                tooltipEl.style.top = positionY + tooltip.caretY + 'px';
                                                tooltipEl.style.padding = tooltip.yPadding + 'px ' + tooltip.xPadding + 'px';
                                                tooltipEl.style.borderRadius = '8px';
                                            },
                                            callbacks: {
                                                title: function (tooltipItem, data) {
                                                    return tooltipItem[0]['xLabel'] + ". hodina";
                                                }
                                                ,
                                                label: function (tooltipItem, data) {


                                                    return tooltipItem.yLabel;
                                                }
                                            }
                                        },
                                        scales: {
                                            xAxes: [{
                                                    ticks: {
                                                        maxRotation: 0,
                                                        autoSkip: false,
                                                    },
                                                    scaleLabel: {
                                                        display: true,
                                                        labelString: myAxisXlabel,
                                                    },
                                                    gridLines: {
                                                        display: false,
                                                    }
                                                }],
                                            yAxes: [{
                                                    id: 'first-y-axis',
                                                    type: 'linear',
                                                    position: 'left',
                                                    ticks: {
                                                        suggestedMin: axisLimits[0]["suggestedMin"],
                                                        suggestedMax: axisLimits[0]["suggestedMax"],
                                                        stepSize: axisLimits[0]["stepSize"],
                                                        callback: function (value, index, values) {
                                                            var space = "";
                                                            if (value >= 0) {
                                                                space = " ";
                                                            }
                                                            return space + formatNumber(value, 0, ',', ' ');
                                                        }
                                                    },
                                                    scaleLabel: {
                                                        display: true,
                                                        labelString: myAxisYlabels[0],
                                                    }
                                                }, {
                                                    id: 'second-y-axis',
                                                    type: 'linear',
                                                    position: 'right',
                                                    ticks: {
                                                        suggestedMin: axisLimits[1]["suggestedMin"],
                                                        suggestedMax: axisLimits[1]["suggestedMax"],
                                                        stepSize: axisLimits[1]["stepSize"],
                                                        //beginAtZero: true,
                                                        callback: function (value, index, values) {
                                                            return formatNumber(Math.floor(value), 0, ',', ' ');
                                                        }
                                                    },
                                                    scaleLabel: {
                                                        display: true,
                                                        labelString: myAxisYlabels[1],
                                                    },
                                                    gridLines: {
                                                        display: false,
                                                    }
                                                }
                                            ]
                                        },
                                        pan: {
                                            enabled: true,
                                            mode: 'xy',
                                        },
                                        zoom: {
                                            enabled: true,
                                            mode: 'xy',
                                        },
                                    }
                                });


                                myData.forEach(function (axisObject, axisIndex) {
                                    var tempObject = axisTemplates[axisIndex];
                                    tempObject['data'] = axisObject;
                                    tempObject['yAxisID'] = myAxisIds[axisIndex];
                                    tempObject['label'] = myAxisYnames[axisIndex];
                                    myChart.data.datasets.push(tempObject);
                                });
                                myChart.update();

                                updateDataset = function (e, datasetIndex) {
                                    var index = datasetIndex;
                                    var ci = myChart;
                                    var meta = ci.getDatasetMeta(index);
                                    meta.hidden = meta.hidden === null ? !ci.data.datasets[index].hidden : null;
                                    var axisLabel = $("#chartjs-legend>ul>li[data-datasetIndex=" + datasetIndex + "]>.axis-name");
                                    if (!meta.hidden) {
                                        axisLabel.css('text-decoration', 'none');
                                    } else {
                                        axisLabel.css('text-decoration', 'line-through');
                                    }

                                    ci.update();
                                };
                                
                                $("#chartjs-legend").html(myChart.generateLegend());
                            }
                        </script>
    </div>

